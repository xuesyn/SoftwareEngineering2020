<view><view hidden="{{!(!hasUserInfo)}}" class="header"><text class="title">Golden Point</text><text class="content">Challenge yourself</text></view><view hidden="{{!(hasUserInfo)}}" class="userinfo"><image class="userinfo-avatar" src="{{Avatar}}" mode="cover"></image><text class="userinfo-nickname">{{nickname}}</text></view><uni-popup class="vue-ref" vue-id="22c086fa-1" type="dialog" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('22c086fa-2')+','+('22c086fa-1')}}" mode="input" placeholder="输入房间Id" title="输入" data-event-opts="{{[['^confirm',[['check']]]]}}" bind:confirm="__e" bind:__l="__l"></uni-popup-dialog></uni-popup><view class="bottom-box"><button hidden="{{!(!hasUserInfo)}}" class="room-bottom" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">授权登录</button></view><view hidden="{{!(hasUserInfo)}}" class="maincontent"><text class="title">新建房间</text><input class="{{[newRoomName==''?'input-notice uni-input':'uni-input']}}" placeholder="请输入房间名称" data-event-opts="{{[['input',[['onInputRoomName',['$event']]]]]}}" value="{{newRoomName}}" bindinput="__e"/><view class="bottom-box"><text data-event-opts="{{[['tap',[['open']]]]}}" class="room-bottom" bindtap="__e">{{"创建新房间"+''}}</text></view><text class="title">进入房间</text><view class="room-container"><scroll-view scroll-y="{{true}}"><view class="room-box"><block wx:for="{{roomList}}" wx:for-item="room" wx:for-index="key" wx:key="roomId"><text data-event-opts="{{[['tap',[['onSelectRoom',['$0'],[[['roomList','roomId',room.roomId]]]]]]]}}" class="room-box-item" bindtap="__e">{{room.name+''}}</text></block></view></scroll-view></view></view></view>